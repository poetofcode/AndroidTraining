<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main_graph"
    app:startDestination="@id/commandFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.example.composeexample02.SplashFragment"
        android:label="SplashFragment" >
        <action
            android:id="@+id/action_splash_to_onboard"
            app:destination="@id/onboardFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/main_graph"
            app:popUpToInclusive="true" />
    </fragment>

    <fragment
        android:id="@+id/onboardFragment"
        android:name="com.example.composeexample02.OnboardFragment"
        android:label="OnboardFragment" />
    <fragment
        android:id="@+id/catalogFragment"
        android:name="com.example.composeexample02.CatalogFragment"
        android:label="CatalogFragment" />
    <fragment
        android:id="@+id/shimmerFragment"
        android:name="com.example.composeexample02.ShimmerFragment"
        android:label="Shimmer" />
    <fragment
        android:id="@+id/pendingWidgets"
        android:name="com.example.composeexample02.PendingWidgets"
        android:label="PendingWidgets" />
    <fragment
        android:id="@+id/tabColumnFragment"
        android:name="com.example.composeexample02.TabColumnFragment"
        android:label="TabColumnFragment" />
    <fragment
        android:id="@+id/sliderFragment"
        android:name="com.example.composeexample02.SliderFragment"
        android:label="SliderFragment" />
    <fragment
        android:id="@+id/galleryFragment"
        android:name="com.example.composeexample02.GalleryFragment"
        android:label="GalleryFragment" />
    <fragment
        android:id="@+id/commandFragment"
        android:name="com.example.composeexample02.CommandFragment"
        android:label="CommandFragment" />

</navigation>